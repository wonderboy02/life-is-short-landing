# Supabase
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=eyJhbGci...
SUPABASE_SERVICE_ROLE_KEY=eyJhbGci... # 서버 전용, 절대 클라이언트 노출 금지

# JWT
JWT_SECRET=your-super-secret-key-minimum-32-characters-long-random-string
JWT_EXPIRES_IN=315360000 # 10년 (초 단위)

# 앱 URL (선택 사항)
# Vercel 배포 시 VERCEL_URL, VERCEL_PROJECT_PRODUCTION_URL이 자동으로 사용됩니다.
# 필요한 경우에만 수동으로 설정하세요.
# NEXT_PUBLIC_APP_URL=https://yourdomain.com

# Admin (별도 추가한 경우)
ADMIN_PASSWORD=your-admin-password

# Open ai api key
OPENAI_API_KEY=

# 베타 테스트 모드 (true일 경우 리뷰 및 가격 섹션 숨김)
NEXT_PUBLIC_IS_BETA_TEST=true

# Worker API 인증 키 (32자 이상)
WORKER_API_KEY=

# gemini ai api key
GEMINI_API_KEY=

# slack integration
SLACK_WEBHOOK_URL=

# kakao app key
NEXT_PUBLIC_KAKAO_JAVASCRIPT_KEY=